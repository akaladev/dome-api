<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping package="com.dome.base.model" >

    <class name="Instruction"
           table="instruction"
           lazy="true">

        <!-- Common id property. -->
        <id name="id"
            type="long"
            column="id"
                >
            <generator class="native"/>
        </id>

        <property   name="dbName"
                    type="string"
                    column="dbName">
        </property>
        
        <property   name="instruction"
                    type="string"
                    column="instruction">
        </property>
        
        <property   name="image"
                    type="string"
                    column="image">
        </property>
        
       

    </class>

</hibernate-mapping>