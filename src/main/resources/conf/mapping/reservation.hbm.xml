<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping package="com.dome.base.model" >

    <class name="Reservation"
           table="reservation"
           lazy="false">

        <!-- Common id property. -->
        <id name="id"
            type="long"
            column="id"
                >
            <generator class="native"/>
        </id>

        <property   name="startdate"
                    type="timestamp"
                    column="start_date">
        </property>
        
        <property   name="enddate"
                    type="timestamp"
                    column="end_date">
        </property>
        
        <property   name="reserver"
                    type="string"
                    column="reserver">
        </property>
        
       

    </class>

</hibernate-mapping>