<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping package="com.dome.base.model" >

    <class name="QRCode"
           table="stack"
           lazy="false">

        <!-- Common id property. -->
        <id name="id"
            type="long"
            column="id"
                >
            <generator class="native"/>
        </id>

        <property   name="animalId"
                    type="long"
                    column="animalId">
        </property>

    </class>

</hibernate-mapping>